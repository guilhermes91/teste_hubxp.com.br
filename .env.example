# Chave secreta do Django (não use esta em produção)
SECRET_KEY=django-insecure-example-key-for-development-only

# Configurações de Debug
DEBUG=True

# Configurações do Banco de Dados PostgreSQL
POSTGRES_DB=weather_db
POSTGRES_USER=weather_user
POSTGRES_PASSWORD=weather_pass
DB_HOST=db
DB_PORT=5432

# Configurações do Cache Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Chave da API do OpenWeatherMap (OBRIGATÓRIO: substitua pela sua chave)
OPENWEATHERMAP_API_KEY=openweatermap-api-key